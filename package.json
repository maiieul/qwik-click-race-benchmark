{
  "name": "qwik-click-race-benchmark-monorepo",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "dev:qwik": "pnpm --filter ./apps/qwik dev",
    "build:qwik": "pnpm --filter ./apps/qwik build && pnpm --filter ./apps/qwik build.server",
    "dev:next": "pnpm --filter ./apps/nextjs dev",
    "build:next": "pnpm --filter ./apps/nextjs build"
  }
}
